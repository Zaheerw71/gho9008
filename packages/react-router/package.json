{
  "name": "@vercel/react-router",
  "description": "Vercel platform abstractions for React Router",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "type": "module",
  "module": "./index.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./entry.server": {
      "types": "./entry.server.d.ts",
      "edge": "./edge/entry.server.js",
      "default": "./entry.server.js"
    },
    "./vite": {
      "types": "./vite.d.ts",
      "default": "./vite.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/react-router"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "defaults/"
  ],
  "scripts": {
    "build": "tsc"
  },
  "keywords": [
    "vercel",
    "react",
    "router"
  ],
  "dependencies": {
    "@vercel/static-config": "3.0.0",
    "ts-morph": "12.0.0"
  },
  "devDependencies": {
    "@react-router/dev": "7.1.3",
    "@react-router/node": "7.1.3",
    "@types/node": "20.17.10",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "isbot": "5.1.21",
    "react-dom": "19.0.0",
    "react-router": "7.1.3",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "@react-router/dev": "7",
    "@react-router/node": "7",
    "isbot": "5",
    "react-dom": "19",
    "react-router": "7"
  }
}
